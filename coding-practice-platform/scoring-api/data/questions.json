[
  {
    "id": "two-sum",
    "title": "Two Sum",
    "description": "Given an array of integers and a target, return indices of two numbers that add up to target. Print the two indices separated by a space (smaller index first).",
    "sampleTests": [
      {"input": "[2,7,11,15]\n9", "expected": "0 1"},
      {"input": "[3,2,4]\n6", "expected": "1 2"}
    ],
    "hiddenTests": [
      {"input": "[1,5,3,8]\n9", "expected": "1 3"},
      {"input": "[0,4,3,0]\n0", "expected": "0 3"},
      {"input": "[-1,-2,-3,-4,-5]\n-8", "expected": "2 4"}
    ]
  },
  {
    "id": "reverse-string",
    "title": "Reverse String",
    "description": "Given a string, return it reversed.",
    "sampleTests": [
      {"input": "hello", "expected": "olleh"},
      {"input": "world", "expected": "dlrow"}
    ],
    "hiddenTests": [
      {"input": "OpenAI", "expected": "IAnepO"},
      {"input": "12345", "expected": "54321"},
      {"input": "", "expected": ""}
    ]
  },
  {
    "id": "max-of-three",
    "title": "Max of Three Numbers",
    "description": "Read three integers (one per line) and print the largest of the three.",
    "sampleTests": [
      {"input": "3\n7\n5", "expected": "7"},
      {"input": "10\n10\n9", "expected": "10"}
    ],
    "hiddenTests": [
      {"input": "-1\n-5\n-3", "expected": "-1"},
      {"input": "42\n17\n28", "expected": "42"}
    ]
  },
  {
    "id": "sum-of-digits",
    "title": "Sum of Digits",
    "description": "Given a non-negative integer N, print the sum of its digits.",
    "sampleTests": [
      {"input": "123", "expected": "6"},
      {"input": "1000", "expected": "1"}
    ],
    "hiddenTests": [
      {"input": "0", "expected": "0"},
      {"input": "9999", "expected": "36"}
    ]
  },
  {
    "id": "factorial",
    "title": "Factorial",
    "description": "Given an integer N (0 <= N <= 12), print N!.",
    "sampleTests": [
      {"input": "5", "expected": "120"},
      {"input": "0", "expected": "1"}
    ],
    "hiddenTests": [
      {"input": "1", "expected": "1"},
      {"input": "7", "expected": "5040"}
    ]
  },
  {
    "id": "palindrome-number",
    "title": "Palindrome Number",
    "description": "Given an integer N, print YES if N is a palindrome, otherwise print NO.",
    "sampleTests": [
      {"input": "121", "expected": "YES"},
      {"input": "123", "expected": "NO"}
    ],
    "hiddenTests": [
      {"input": "0", "expected": "YES"},
      {"input": "1221", "expected": "YES"}
    ]
  },
  {
    "id": "count-vowels",
    "title": "Count Vowels",
    "description": "Given a line of text, count the vowels (a, e, i, o, u) ignoring case and print the count.",
    "sampleTests": [
      {"input": "hello", "expected": "2"},
      {"input": "AEIOU", "expected": "5"}
    ],
    "hiddenTests": [
      {"input": "bcd", "expected": "0"},
      {"input": "OpenAI", "expected": "4"}
    ]
  },
  {
    "id": "simple-interest",
    "title": "Simple Interest",
    "description": "Given principal P, rate R, and time T (each on a new line), print the simple interest (P*R*T/100).",
    "sampleTests": [
      {"input": "1000\n5\n2", "expected": "100"},
      {"input": "1500\n10\n3", "expected": "450"}
    ],
    "hiddenTests": [
      {"input": "2000\n7\n1", "expected": "140"},
      {"input": "500\n12\n2", "expected": "120"}
    ]
  },
  {
    "id": "grade-classification",
    "title": "Grade Classification",
    "description": "Given marks (0-100), print A for >=90, B for >=80, C for >=70, D for >=60, else F.",
    "sampleTests": [
      {"input": "85", "expected": "B"},
      {"input": "59", "expected": "F"}
    ],
    "hiddenTests": [
      {"input": "90", "expected": "A"},
      {"input": "70", "expected": "C"}
    ]
  }

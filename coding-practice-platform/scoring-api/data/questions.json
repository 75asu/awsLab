[
  {
    "id": "two-sum",
    "title": "Two Sum",
    "description": "Given an array of integers and a target, return indices of two numbers that add up to target. Print the two indices separated by a space (smaller index first).",
    "sampleTests": [
      {"input": "[2,7,11,15]\n9", "expected": "0 1"},
      {"input": "[3,2,4]\n6", "expected": "1 2"}
    ],
    "hiddenTests": [
      {"input": "[1,5,3,8]\n9", "expected": "1 3"},
      {"input": "[0,4,3,0]\n0", "expected": "0 3"},
      {"input": "[-1,-2,-3,-4,-5]\n-8", "expected": "2 4"}
    ]
  },
  {
    "id": "reverse-string",
    "title": "Reverse String",
    "description": "Given a string, return it reversed.",
    "sampleTests": [
      {"input": "hello", "expected": "olleh"},
      {"input": "world", "expected": "dlrow"}
    ],
    "hiddenTests": [
      {"input": "OpenAI", "expected": "IAnepO"},
      {"input": "12345", "expected": "54321"},
      {"input": "", "expected": ""}
    ]
  }
]
